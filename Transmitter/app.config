<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <udpBinding>
                <binding name="udpBindingStockService" />
            </udpBinding>
        </bindings>
        <client>
            <endpoint binding="udpBinding"
                bindingConfiguration="udpBindingStockService" contract="StockServiceRef.IStockService"
                name="UdpBinding_IStockService" />
        </client>
    </system.serviceModel>

    <appSettings configSource="ProfitCenter.config"/>
</configuration>